{
    "name": "kalakotraamir/silverstripe-bootstrap-scss",
    "description": "Bootstrap theme integration in Silverstripe 4 with minifier",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "axllent/silverstripe-scss": "^1.5",
        "axllent/silverstripe-minifier": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripeModule\\Skeleton\\": "src/",
            "SilverStripeModule\\Skeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
